# Environment Variables for Qwen Messaging Agent
# Copy this file to .env and fill in your values

# Google Cloud Configuration
PROJECT_ID=your-project-id
REGION=us-central1
ENDPOINT_ID=your-endpoint-id

# API Configuration
API_KEY=your-api-key-optional
RATE_LIMIT_PER_MINUTE=60

# Security Configuration
ALLOWED_ORIGINS=https://yourdomain.com,https://app.yourdomain.com
ADMIN_PASSWORD=your-secure-admin-password
JWT_SECRET_KEY=your-jwt-secret-key-min-32-chars
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_SSL=false

# CORS Settings
CORS_ORIGINS=https://yourdomain.com,https://app.yourdomain.com
CORS_METHODS=GET,POST,PUT,DELETE
CORS_HEADERS=Authorization,Content-Type,X-API-Key

# Security Headers
SECURITY_HEADERS_ENABLED=true
STRICT_TRANSPORT_SECURITY=true
CONTENT_SECURITY_POLICY=strict

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379

# BigQuery Configuration
DATASET_ID=messaging_agent
TABLE_ID=conversations

# Optimization Settings
MAX_CONTEXT_TOKENS=2000
ENABLE_CACHING=true
ENABLE_BATCHING=true

# Monitoring
WANDB_PROJECT=qwen-messaging-agent
WANDB_API_KEY=your-wandb-key-optional
LOG_LEVEL=INFO

# Agent Registry (for multi-agent future)
TICKETING_ENDPOINT=projects/${PROJECT_ID}/locations/${REGION}/endpoints/${ENDPOINT_ID}
SALES_ENDPOINT=
FINANCE_ENDPOINT=
HR_ENDPOINT=

# Twilio SMS Integration
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Evaluation System
OPENAI_API_KEY=your-openai-api-key
EVAL_MODEL=gpt-4-turbo-preview
EVAL_PROVIDER=openai
MIN_EVAL_SCORE=0.80
MIN_PASS_RATE=0.85

# Ticket Platform Integrations
STUBHUB_API_KEY=your-stubhub-key
STUBHUB_API_SECRET=your-stubhub-secret
STUBHUB_ENV=production
SEATGEEK_CLIENT_ID=your-seatgeek-client-id
SEATGEEK_CLIENT_SECRET=
TICKETMASTER_API_KEY=your-ticketmaster-key
